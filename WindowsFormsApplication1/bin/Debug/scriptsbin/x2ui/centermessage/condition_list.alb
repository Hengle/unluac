function GetContentStr(content)
  if content == UIC_RANK then
    return X2Locale:LocalizeUiText(RANKING_TEXT, "ranking")
  elseif content == UIC_CHARACTER_INFO then
    return X2Locale:LocalizeUiText(COMMON_TEXT, "character_main_widget_title")
  elseif content == UIC_BAG then
    return X2Locale:LocalizeUiText(INVEN_TEXT, "bagTitle")
  elseif content == UIC_QUEST_LIST then
    return X2Locale:LocalizeUiText(QUEST_TEXT, "widget_title")
  elseif content == UIC_ACHIEVEMENT then
    return X2Locale:LocalizeUiText(COMMON_TEXT, "assignment_system")
  elseif content == UIC_SKILL then
    return X2Locale:LocalizeUiText(SKILL_TEXT, "titleText")
  elseif content == UIC_WORLDMAP then
    return X2Locale:LocalizeUiText(MAP_TEXT, "title")
  elseif content == UIC_CRAFT_BOOK then
    return X2Locale:LocalizeUiText(CRAFT_TEXT, "craftBook")
  elseif content == UIC_MY_FARM_INFO then
    return X2Locale:LocalizeUiText(MAP_TEXT, "M_COMMON_FARM")
  elseif content == UIC_EXPEDITION then
    return X2Locale:LocalizeUiText(COMMUNITY_TEXT, "expedition")
  elseif content == UIC_FRIEND then
    return X2Locale:LocalizeUiText(COMMUNITY_TEXT, "relationship")
  elseif content == UIC_NATION then
    return X2Locale:LocalizeUiText(NATION_TEXT, "nation")
  end
  return nil
end
