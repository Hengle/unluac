function GetLevelUpConditionInfo(level)
  for i = 1, #conditionList do
    local info = conditionList[i]
    if info.condition == CONDITION_LEVEL_UP and info.value == level then
      return info
    end
  end
  return nil
end
function GetQuestCompleteConditionInfo(qType)
  for i = 1, #conditionList do
    local info = conditionList[i]
    if info.condition == CONDITION_QUEST_COMPLETE and info.value == qType then
      return info
    end
  end
  return nil
end
